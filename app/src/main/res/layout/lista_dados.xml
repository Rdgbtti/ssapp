<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

   <TextView
       android:id="@+id/txtData"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_margin="20dp"
       android:text="14/10/2023"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/txtHora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="kljhkljklj"
        android:layout_marginStart="140dp"
        app:layout_constraintStart_toEndOf="@id/txtData"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/txtDadoUm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="kljhkljklj"
        android:layout_marginStart="140dp"
        app:layout_constraintStart_toEndOf="@id/txtHora"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/txtDadoDois"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="kljhkljklj"
        android:layout_marginStart="140dp"
        app:layout_constraintStart_toEndOf="@id/txtDadoUm"
        app:layout_constraintTop_toTopOf="parent"
        />
    <ImageView
        android:id="@+id/btnEditar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:src="@drawable/ic_editar"
        android:layout_marginStart="140dp"
        app:layout_constraintStart_toEndOf="@id/txtDadoDois"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/btnExcluir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:src="@drawable/ic_excluir"
        android:layout_marginStart="140dp"
        app:layout_constraintStart_toEndOf="@id/btnEditar"
        app:layout_constraintTop_toTopOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>